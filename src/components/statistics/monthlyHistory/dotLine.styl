.dot-line
  padding 0 16px 16px
  svg
    overflow hidden
  rect
    fill #f4f4f4
    stroke #ddd
    stroke-width .5
  path
    stroke rgba(200,40,41,.6)
    stroke-width 2.5px
    fill none
  text
    fill rgba(200,40,41,.6)
    font-size 1rem
  circle
    stroke rgba(200,40,41,.8)
    fill #fff
    stroke-width 2px
